<div class="mt-4">
    <div class="row">
        {{#each courses}}
        <div class="col-sm-6 col-lg-4 mt-16">
            <div class="card">
                <img src={{this.image}} class="card-img-top" alt="{{this.name}}">
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                    <p class="card-text">{{this.description}}</p>
                    <a href="#" class="btn btn-primary">Study</a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>